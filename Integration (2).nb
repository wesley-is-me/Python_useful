Cell["\<\
This block of code helps you visualize setting up bounds in terms of \
\"Smashing\" or \"Constructing.\" The lines drawn in each diagram represent \
the lines related to the inner variable of integration.  You have to smash \
the inner variable of integration out of a problem before you can smash the \
outer variable of integration out. \
\>", "Text",
 ShowGroupOpener->True,
 CellChangeTimes->{{3.4459135753939314`*^9, 
  3.44591367355508*^9}},ExpressionUUID->"4685eccd-0112-4cfd-a3f1-\
304d6f29009a"],

Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"x", ",", "y"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"OuterBounds", "=", 
   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"-", "3"}], ",", "3"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"InnerBounds", "=", 
    RowBox[{"{", 
     RowBox[{"y", ",", "0", ",", 
      RowBox[{"x", "^", "2"}]}], "}"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rT", "=", 
   RowBox[{"{", 
    RowBox[{"x", ",", "y"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"DT", "=", 
   RowBox[{"D", "[", 
    RowBox[{"rT", ",", 
     RowBox[{"{", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"OuterBounds", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{"InnerBounds", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], "}"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Jacob", "=", 
    RowBox[{"Abs", "[", 
     RowBox[{"Det", "[", "DT", "]"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"Manipulate", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"rr", "=", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x", ",", "y"}], "}"}], "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x", "\[Rule]", 
         RowBox[{"Coordinates", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ",", 
        RowBox[{"y", "\[Rule]", 
         RowBox[{"Coordinates", "[", 
          RowBox[{"[", "2", "]"}], "]"}]}]}], "}"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"rs", "=", 
     RowBox[{"rr", "/.", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"OB", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"OB", "[", 
             RowBox[{"[", "1", "]"}], "]"}], 
            RowBox[{"(", 
             RowBox[{"1", "-", "OBSmash"}], ")"}]}], "+", 
           RowBox[{"OBSmash", " ", "SmashOBTo"}]}], ")"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"IB", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"IB", "[", 
             RowBox[{"[", "1", "]"}], "]"}], 
            RowBox[{"(", 
             RowBox[{"1", "-", "IBSmash"}], ")"}]}], "+", 
           RowBox[{"IBSmash", " ", "SmashIBTo"}]}], ")"}]}]}], "}"}]}]}], ";",
     "\[IndentingNewLine]", 
    RowBox[{"wIB", "=", "IB"}], ";", 
    RowBox[{"wOB", "=", "OB"}], ";", 
    RowBox[{"wMB", "=", "MB"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Show", "[", 
     RowBox[{"{", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"ParametricPlot", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"rr", "/.", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"wIB", "[", 
               RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{
                 RowBox[{"wIB", "[", 
                  RowBox[{"[", "2", "]"}], "]"}], 
                 RowBox[{"(", 
                  RowBox[{"1", "-", "s"}], ")"}]}], "+", 
                RowBox[{
                 RowBox[{"wIB", "[", 
                  RowBox[{"[", "3", "]"}], "]"}], "s"}]}], ")"}]}], ")"}]}], 
           ")"}], ",", 
          RowBox[{"Evaluate", "[", "wOB", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"s", ",", "0", ",", "1"}], "}"}], ",", 
          RowBox[{"MeshShading", "\[Rule]", "None"}], ",", 
          RowBox[{"Mesh", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{"21", ",", "0"}], "}"}]}], ",", 
          RowBox[{"PlotStyle", "\[Rule]", "None"}], ",", 
          RowBox[{"PlotRange", "\[Rule]", "All"}]}], "]"}], ",", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"ParametricPlot", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"rs", "/.", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"wIB", "[", 
               RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{
                 RowBox[{"wIB", "[", 
                  RowBox[{"[", "2", "]"}], "]"}], 
                 RowBox[{"(", 
                  RowBox[{"1", "-", "s"}], ")"}]}], "+", 
                RowBox[{
                 RowBox[{"wIB", "[", 
                  RowBox[{"[", "3", "]"}], "]"}], "s"}]}], ")"}]}], ")"}]}], 
           ")"}], ",", "\[IndentingNewLine]", 
          RowBox[{"Evaluate", "[", "wOB", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"s", ",", "0", ",", "1"}], "}"}], ",", 
          RowBox[{"PlotStyle", "\[Rule]", "Blue"}], ",", 
          RowBox[{"Mesh", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{"21", ",", "0"}], "}"}]}], ",", 
          RowBox[{"PlotRange", "\[Rule]", "All"}]}], "]"}]}], 
       "\[IndentingNewLine]", "}"}], "}"}], "]"}]}], "\[IndentingNewLine]", 
   ",", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{"Coordinates", ",", "rT"}], "}"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{"OB", ",", "OuterBounds"}], "}"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{"IB", ",", "InnerBounds"}], "}"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"SmashIBTo", ",", "0"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"IBSmash", ",", "0", ",", "1"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"SmashOBTo", ",", "0"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"OBSmash", ",", "0", ",", "1"}], "}"}], ",", 
   RowBox[{"ControlPlacement", "\[Rule]", "Left"}]}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Grid", "[", 
  RowBox[{
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {"\"\<Transformation\>\"", "rT"},
      {"\"\<Derivative\>\"", 
       RowBox[{"DT", "//", "MatrixForm"}]},
